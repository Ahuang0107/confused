{
  "name": "confused",
  "version": "1.0.1",
  "description": "a little ui lib",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build:esm": "cross-env BABEL_ENV=esm babel src --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:umd": "cross-env NODE_ENV=umd webpack",
    "build:umd:min": "cross-env NODE_ENV=umd:min webpack",
    "build:iife": "cross-env NODE_ENV=iife webpack",
    "build:iife:min": "cross-env NODE_ENV=iife:min webpack",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build": "yarn run build:esm & yarn run build:cjs & yarn run build:umd & yarn run build:umd:min & yarn run build:iife & yarn run build:iife:min & yarn run build:types",
    "prepublish": "yarn run build",
    "publish": "yarn npm publish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "elase.huang",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "babel-loader": "^8.2.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  }
}
